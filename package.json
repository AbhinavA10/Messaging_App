{
  "name": "messaging_app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "remote_android": "expo start --tunnel",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "predeploy": "expo export -p web",
    "deploy_github": "npm run predeploy && gh-pages -t -d dist",
    "deploy_firebase": "npm run predeploy && firebase deploy --only hosting",
    "deploy_local": "npm run predeploy && npx serve dist"
  },
  "dependencies": {
    "@expo/metro-runtime": "^3.1.3",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "expo": "^50.0.17",
    "expo-status-bar": "~1.11.1",
    "expo-updates": "~0.24.12",
    "firebase": "^10.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "gh-pages": "^6.1.1"
  },
  "private": true,
  "homepage": "http://ashisha26.github.io/Messaging_App"
}
